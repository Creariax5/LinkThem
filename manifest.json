{
    "manifest_version": 3,
    "name": "LinkedinScrap",
    "version": "1.0.0",
    "description": "Srap your linkedin to get all infos you need",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "downloads"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}